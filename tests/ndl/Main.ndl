link Link {
    bitrate: 10000
    latency: 0.1
    jitter: 0.1
}

module Bob {
    submodules:
        child: Alice

    gates:
        netIn[3]
        netOut[3]
        uplink

    connections:
        netOut --> Link --> child/netIn
        netIn <-- Link <-- child/netOut
}

module Alice {
    gates:
        netIn[3]
        netOut[3]
}


network A {
    nodes:
        bob[1...100]: Bob
}
