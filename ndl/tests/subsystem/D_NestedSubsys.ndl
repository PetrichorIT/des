module A {
    gates:
        in @input
        out @output
}

subsystem Sub {
    nodes:
        self: A

    exports:
        self/in
        self/out
}

subsystem Main {
    nodes:
        module: A
        subsys: Sub

    connections:
        module/in <-- subsys/out
        module/out --> subsys/in
}

subsystem FieldError {
    nodes:
        module: A
        subsys: Sub

    connections:
        module/in <-- error/out
        module/out --> subsys/in
}

subsystem GateError {
    nodes:
        module: A
        subsys: Sub

    connections:
        module/in <-- subsys/error
        module/out --> subsys/in
}