module M {
    gates:
        one_in @input
        one_out @output
        cluster[5]
}

network Sub {
    nodes:
        a: M
        b: M
        c: M

    exports:
        a/one_in
        b/one_out
        c/cluster
}

prototype P {
    gates:
        in @input
        out @output
}

alias SomeP like P
alias OtherP like P

network Main {
    nodes:
        sub: Sub
        m: M
}