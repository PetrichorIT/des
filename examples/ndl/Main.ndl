link Link {
    bitrate: 10000,
    latency: 0.1,
    jitter: 0.1,
}

module Bob {
    submodules {
        child: Alice
    }

    gates {
        netIn[3] @input,
        netOut[3] @output,
        uplink,
    }

    connections {
        netOut --> Link --> child/netIn,
        netIn <-- Link <-- child/netOut,
    }
}

module Alice {
    gates {
        netIn[3],
        netOut[3],
    }
}


module A {
    submodules {
        bob[100]: Bob
    }
}

entry A;