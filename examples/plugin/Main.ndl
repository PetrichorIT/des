module A {
    gates:
        in @input
        out @output
}

module B {
    gates:
        in @input
        out @output
}

link L {
    jitter: 0.0
    latency: 0.05
    bitrate: 100000000
    queuesize: 2000
}

subsystem Main {
    nodes:
        a: A
        b: B
    
    connections:
        a/out --> L --> b/in
        b/out --> L --> a/in
}