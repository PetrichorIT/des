link Link {
    bitrate: 100
    latency: 0.1
    jitter: 0.1
}

module Bob {
    submodules:
        child: Alice

    gates:
        netIn[3]
        netOut[3]
        uplink

    connections:
        netOut --> Link --> child/netIn
        netIn <-- Link <-- child/netOut
}

module Alice {
    gates:
        netIn[3]
        netOut[3]
}

module Eve {
    gates:
        link
}

network A {
    nodes:
        router: Eve
        bob[1...5]: Bob

    connections:
        bob/uplink --> Link --> router/link
}