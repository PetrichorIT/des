include A
include std/A

link NewLink {
    bitrate: 300
    jitter: 0.1
    latency: 0.1
}

module SubM {
    parameters:
        addr: usize

    gates:
        another[5]
}

module Main {
    submodules:
        m: SubM

    gates:
        some[5]
        same[5]
        sike

    connections:
        some --> m/another
        same <-- NewLink <-- sike 
}