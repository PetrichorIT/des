module TopLevelModule {
    submodules:
        a: MidLevelModule
        b: MidLevelModule
}

module MidLevelModule {
    submodules:
        child: LowLevelModule

    gates:
        in @input
        out @output

    connections:
        out --> child/in
        in <-- child/out
}

module LowLevelModule {
    gates:
        in @input
        out @output
}

subsystem TestCase {
    nodes:
        root: TopLevelModule
}